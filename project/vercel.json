{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "public": true,
  "cleanUrls": true,
  "regions": ["sin1"],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ],
  "env": {
    "IS_WEBCONTAINER": "false"
  }
}
